        { "comment": "13", "header": "Header-14", "targetType": "Float64" },
        { "comment": "14", "header": "Header-15", "targetType": "Bool" },
        { "comment": "15", "header": "Header-16", "targetType": "Decimal" },        
        { "comment": "16", "header": "Header-17", "targetType": "NaiveDate", "srcPattern": "%Y %m %d"},
        { "comment": "17", "header": "Header-18", "targetType": "NaiveDateTime", "srcPattern": "%Y-%m-%d %H:%M:%S"},
